<% if(@warehouse.errors.any?) %>
	<ul>
	<% @warehouse.errors.full_messages.each do | msg| %>
		<li><%= msg %> </li>
	<% end %>
	</ul>
<% end %>

<%= form_with(model: @warehouse) do |form| %>
	<div>
		<%= form.label :name%>
		<%= form.text_field :name%>
	</div>
	<div>
		<%= form.label :code%>
		<%= form.text_field :code%>
	</div>
	<div>
		<%= form.label :city%>
		<%= form.text_field :city%>
	<div>
		<%= form.label :area%>
		<%= form.text_field :area%>
	</div>
	<div>
		<%= form.label :address%>
		<%= form.text_field :address%>
	</div>
	<div>
		<%= form.label :postal_code, 'Cep'%>
		<%= form.text_field :postal_code%>
	</div>
	<div>
		<%= form.label :description%>
		<%= form.text_area :description%>
	</div>
	<div>
		<%= form.submit 'Salvar'%>
	</div>
<% end %>